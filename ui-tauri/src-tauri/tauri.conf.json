{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "productName": "ezlog-ui",
  "mainBinaryName": "ezlog-ui-binary",
  "version": "0.1.0",
  "identifier": "wtf.s1.ezlog.tauri",
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ezlog",
        "theme": null,
        "width": 800,
        "height": 600,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}